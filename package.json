{
  "name": "react-project-tmplate",
  "version": "1.0.0",
  "description": "A react scaffold",
  "author": "duuliy (715181149@qq.com)",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack serve --progress --config webpack.config.js",
    "build": "cross-env CLIENT_ENV=production NODE_ENV=production webpack --config webpack.config.js",
    "test": "rimraf coverage && cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "jest": {
    "collectCoverage": false,
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    },
    "collectCoverageFrom": [
      "src/pages/**/*.{js,jsx,ts,tsx}",
      "!src/pages/**/*.test.{js,jsx,ts,tsx}",
      "src/components/**/*.{js,jsx,ts,tsx}",
      "!src/components/**/*.test.{js,jsx,ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.config.js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)?$": "babel-jest",
      "^.+\\.css$": "<rootDir>/testBuild/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/testBuild/fileTransform.js"
    },
    "testPathIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/testBuild/fileMock.js",
      "\\.(css|less)$": "<rootDir>/testBuild/styleMock.js",
      "@utils/(.*)": "<rootDir>/src/utils/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@components/(.*)": "<rootDir>/src/components/$1",
      "@public/(.*)": "<rootDir>/public/$1",
      "@/(.*)": "<rootDir>/src/$1"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "sideEffects": false,
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "antd": "^5.0.1",
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-import": "^1.13.5",
    "classnames": "^2.3.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "cssnano": "^5.1.14",
    "enzyme": "^3.11.0",
    "file-loader": "^6.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "immer": "^9.0.16",
    "jest": "^29.3.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lodash": "^4.17.21",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^2.7.0",
    "mocker-api": "^2.9.5",
    "moment": "^2.29.4",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "^7.0.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-hot-loader": "^4.13.1",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^3.0.3",
    "ts-jest": "^29.0.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3"
  }
}
